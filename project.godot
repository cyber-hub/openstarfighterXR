EffectSpawner="*res://src/autoloads/effect_spawner.gd"
UserPreferences="*res://src/autoloads/user_preferences.gd"
ShipRegistry="*res://src/autoloads/ship_registry.gd"
SceneSwitcher="*res://src/autoloads/scene_switcher/scene_switcher.tscn"
XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
Globals="*res://src/autoloads/globals.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Space Fighter Template"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg")

